#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#define N 3
#define M 3

int main(void)
{   int i,j,k,min;
	int matrix1[N][M];
	int matrix2[N][M];
	int temp[N];
	FILE *fin;
	FILE *fout;
	fin=fopen("input.txt","rt");
	if(fin == NULL)
		{   perror ("FATAL ERROR");
			return -1;
		}
	for(i=0; i!=N;i++)
	{
		for(j=0; j!= M;j++)
		{   
			fscanf(fin,"%d",&matrix1[i][j]);
		}
	}
	fclose(fin);	

	for(i=0;i!=N;i++)
		{
			for(j=0;j!=M;j++)
			matrix2[i][j]=matrix1[i][j];
		}
	min=matrix2[0][0];
	i=1;

	for(k=0;k!=N;k++)
		{
			if(matrix2[i][0]<matrix2[i-1][0])
				{
					k=0;
					for(j=0;j<M;j++)
						temp[j]=matrix2[i][j];
					for(j=0;j<M;j++)
						matrix2[i][j]=matrix2[i-1][j];
					for(j=0;j<M;j++)
						matrix2[i-1][j]=temp[j];
					i=1;
				}
			else i++;
		}
	
	fout=fopen("output.txt","w");
	if(fout == NULL)
	{	
		perror("Error open file");
		return -1;
	}
	for(i=0;i!=N;i++)
		{
			for(j=0;j!=M;j++)
			{
				fprintf(fout,"%d ",matrix2[i][j]);
			}
			fprintf(fout,"\n");
		}
	fclose(fout);
	return 0;
	}

