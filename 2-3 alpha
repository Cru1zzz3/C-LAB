#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <math.h>

#define N 3
#define M 3


int input(int *a,int n,int m);
int output(int  *a,int n,int m);

int main(void)
{
	int A[N][M];
	input(A);
	output(A);
	return 0;
}

int input(int (*a)[M])
{   int i,j;
	printf("Input array:\n");
	for (i=0;i<N;i++)
	{
		for (j=0;j<M;j++)
		{
			scanf("%с",&a[i][j]);
		}
	}
	return 0;
}

int output(int  (*a)[M])
{
	int i,j;
	for (i=0;i<N;i++)
	{
		for (j=0;j<M;j++)
		{
			printf("%c",a[i][j]);
		}
		printf("\n");
	}
	return 0;
}
