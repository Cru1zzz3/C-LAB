#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#define N 15 
#define k = 0
void input(char *str, int n);
void output(char *str, int n);
int i, j = 0;
int main()
{
	char str1[N], str2[N];
	char min;
	int n = N;
	printf("Input string:\n");
	input(&str1[0], n);	
	for (i = 0;i < n;i++)
	{
		str2[i] = str1[i];
	}
	 min = str2[0];
	for (i = 0;i<n;i++)
	{
		if (str2[i] >= min)
		{
			str1[j] = str2[i];
			min=str2[i];
			j++;
			
		}
	}
	output(&str1[0], n);
	return 0;
}

void input(char *str, int n)
{	
	for (i = 0;i<n;i++)
	scanf("%c", &str[i]);
}

void output(char *str, int n)
{
	for (i = 0;i!=j;i++)
	printf("%c", str[i]);
	printf("\n");
}
