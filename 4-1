#include <stdio.h>
#include <time.h>
#include <stdlib.h>
#include <malloc.h>
#include <string.h>


#define foodCount 100

typedef struct {
	int hp;
	int power;
	int position[2];  
} TDove;

typedef struct {
	int hp;
	int power;
	int position[2];  
} THawk;

int bornDove(TDove *doves,int doveCount){
	doves=(TDove*)realloc(doves,(doveCount+1)*sizeof(TDove));
	memset(doves[doveCount].hp,100,sizeof(doves[doveCount].hp));
	doveCount++;
	return doveCount;
}

void bornHawk(THawk *hawks,int *hawkCount){
	hawks=(THawk*)realloc(hawks,(*hawkCount+1)*sizeof(THawk));
	*hawkCount++;
}



void deathDove(TDove *doves,int *doveCount){
	doves=(TDove*)realloc(doves,(*doveCount-1)*sizeof(TDove));
	*doveCount--;
}

void deathHawk(THawk *hawks,int *hawkCount){
	hawks=(THawk*)realloc(hawks,(*hawkCount-1)*sizeof(THawk));
	*hawkCount--;
}

void main(void)
{
	TDove *doves=NULL;
	THawk *hawkp=NULL;
	int i;
	int doveCount=0;
	int hawkCount=0;
	for (i=0;i<5;i++)
	bornDove(doves,doveCount);
	printf("%d", doveCount);
	getchar();	

}
